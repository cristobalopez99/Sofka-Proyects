Proceso capitalCities
	
	Definir capitalCity Como Caracter;
	Definir country Como Caracter;
	
	Escribir "Please write a capital city ____";
	Leer capitalCity;
	
	Escribir "Please write the country of that capital city ___ ";
	Leer country;
	Limpiar Pantalla;
	
	Escribir "The city ", capitalCity, " is the capital city of the country ", country;
	
FinProceso
